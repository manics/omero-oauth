config set omero.web.oauth.host localhost
config set omero.web.oauth.admin.user root
config set omero.web.oauth.admin.password omero

config set omero.web.oauth.group.name oauth
config set omero.web.oauth.providers /opt/omero/web/config/oauth-providers.yaml

# Change the default login page to oauth
config set omero.web.login_view oauth_index
